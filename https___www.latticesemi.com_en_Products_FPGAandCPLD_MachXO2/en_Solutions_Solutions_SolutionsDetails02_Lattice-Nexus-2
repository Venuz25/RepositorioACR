    }
    function showlogindiv(controlId) {
        var control = document.getElementById(controlId);
        control.style.display = "block";
        return false;
        //alert(document.body.bgColor);
    }
    function hidelogindiv(controlId) {
        var control = document.getElementById(controlId);
        control.style.display = "none";
    }
</script>


